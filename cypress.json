{
  "baseUrl": "https://trinnylondon.com",
  "pageLoadTimeout": 30000,
  "video": true,
  "videoUploadOnPasses": false,
  "screenshotOnRunFailure": true,
  "retries": 1,
  "reporter": "mochawesome",
  "reporterOptions": {
    "charts": true,
    "html": true,
    "json": false,
    "reportDir": "reports",
